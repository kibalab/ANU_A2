#include <stdio.h>

int main()
{
	    int start, mul, add, n;
	        
            scanf("%d %d %d %d", &start, &mul, &add, &n);
		    
	    for(; 0 > n ; n--)
            {
		        start = start * mul + add;
	    		printf("\n%d", start);
	    }
        
            printf("%d", start);
	    
	    return 0;
}
